{
    "expo": {
        "name": "Инвентаризация",
        "slug": "Inventory",
        "version": "1.1",
        "orientation": "portrait",
        "icon": "./assets/inventory.png",
        "splash": {
            "image": "./assets/invSplash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/inventory.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.inventory.appname",
            "permissions": [
                "CAMERA",
                "READ_EXTERNAL_STORAGE",
                "WRITE_EXTERNAL_STORAGE",
                "MEDIA_LIBRARY"
            ]
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "description": "Company inventory",
        "githubUrl": "https://github.com/iliapnmrv/inventory"
    }
}